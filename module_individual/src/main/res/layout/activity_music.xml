<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/individual_head_background"
    tools:context=".MusicActivity">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/socializing_music_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="取消"
            android:textColor="#000"
            android:textSize="24dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.529" />

        <TextView
            android:id="@+id/socializing_music_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="文化曲乐"
            android:textColor="#000"
            android:textSize="24dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/socializing_music_back"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/socializing_music_back"
            app:layout_constraintVertical_bias="0.454" />

    </androidx.constraintlayout.widget.ConstraintLayout>


    <ImageView
        android:id="@+id/individual_music_start"
        android:layout_width="48dp"
        android:layout_height="55dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.718"
        app:srcCompat="@drawable/individual_music_start" />

    <ImageView
        android:id="@+id/individual_music_image"
        android:layout_width="334dp"
        android:layout_height="334dp"
        android:layout_marginTop="92dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
        app:srcCompat="@drawable/individual_music_image1" />

    <TextView
        android:id="@+id/individual_music_name"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        android:layout_marginTop="24dp"
        android:text="黄梅戏"
        android:textColor="#000"
        android:textSize="24dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2" />

    <TextView
        android:id="@+id/individual_music_writer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="吴琼/慕容晓晓"
        app:layout_constraintEnd_toEndOf="@+id/individual_music_name"
        app:layout_constraintStart_toStartOf="@+id/individual_music_name"
        app:layout_constraintTop_toBottomOf="@+id/individual_music_name" />

    <ImageView
        android:id="@+id/individual_music_right"
        android:layout_width="48dp"
        android:layout_height="55dp"
        android:layout_marginStart="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/individual_music_start"
        app:layout_constraintStart_toEndOf="@+id/individual_music_start"
        app:layout_constraintTop_toTopOf="@+id/individual_music_start"
        app:layout_constraintVertical_bias="0.478"
        app:srcCompat="@drawable/individual_music_right" />

    <ImageView
        android:id="@+id/individual_music_left"
        android:layout_width="48dp"
        android:layout_height="55dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/individual_music_start"
        app:layout_constraintEnd_toStartOf="@+id/individual_music_start"
        app:layout_constraintTop_toTopOf="@+id/individual_music_start"
        app:layout_constraintVertical_bias="0.478"
        app:srcCompat="@drawable/individual_music_left" />

    <ImageView
        android:id="@+id/individual_music_love"
        android:layout_width="48dp"
        android:layout_height="55dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/individual_music_left"
        app:layout_constraintEnd_toStartOf="@+id/individual_music_left"
        app:layout_constraintTop_toTopOf="@+id/individual_music_left"
        app:srcCompat="@drawable/individual_music_love" />

    <ImageView
        android:id="@+id/individual_music_list"
        android:layout_width="48dp"
        android:layout_height="55dp"
        android:layout_marginStart="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/individual_music_right"
        app:layout_constraintStart_toEndOf="@+id/individual_music_right"
        app:layout_constraintTop_toTopOf="@+id/individual_music_right"
        app:srcCompat="@drawable/individual_music_list" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="免费歌曲列表"
        android:textColor="#2c2c2c"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/individual_music_love"
        app:layout_constraintTop_toBottomOf="@+id/individual_music_love" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="春江花月夜-中央民族乐团"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        android:text="浏阳河—红歌100首"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />


</androidx.constraintlayout.widget.ConstraintLayout>